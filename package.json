{
  "name": "vue-vk",
  "version": "0.0.1",
  "description": "Provides a simple layer on top of the VK widget iframe api.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "build": "WEBPACK_ENV=production webpack",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "lint:travis": "travis-lint .travis.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flui/vue-vk.git"
  },
  "keywords": [
    "vue.js",
    "vk"
  ],
  "author": "Florian Schultze <f.schultze@mailbox.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Flui/vue-vk/issues"
  },
  "homepage": "https://github.com/Flui/vue-vk#readme",
  "peerDependencies": {
    "parse-color": "^1.0.0",
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-knobs": "^3.3.13",
    "@storybook/addon-notes": "^3.3.13",
    "@storybook/vue": "^3.3.13",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.3.0",
    "jest-vue-preprocessor": "^1.3.1",
    "regenerator-runtime": "^0.11.1",
    "travis-lint": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "parse-color": "^1.0.0",
    "vue": "^2.5.13"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "jest-vue-preprocessor"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
